{
	"id": "com.sap.helloworld",
	"version": "2.1.0",
	"name": "Hello World test",
	"description": "An Hello World demo",
	"newInstancePrefix": "HelloWorld",
	"icon": "data:image/png;base64,",
	"vendor": "Demo",
	"eula": "",
	"license": "",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "com-sap-helloworld",
			"url": "/HelloWorld.js",
			"integrity": "",
			"ignoreIntegrity": true
		},
		{
			"kind": "styling",
			"tag": "com-sap-helloworld-styling",
			"url": "/HelloWorld_styling.js",
			"integrity": "",
			"ignoreIntegrity": true
		},
		{
			"kind": "builder",
			"tag": "com-sap-helloworld-builder",
			"url": "/HelloWorld_builder.js",
			"integrity": "",
			"ignoreIntegrity": true
		}
	],
	"properties": {
		"color": {
			"type": "string",
			"description": "Background color",
			"default": "white"
		},
		"opacity": {
			"type": "number",
			"description": "Opacity",
			"default": 1
		},
		"width": {
			"type": "integer",
			"default": 100
		},
		"height": {
			"type": "integer",
			"default": 100
		},
		"title" : {
			"type": "string",
			"description": "Title",
			"default": "Title"
		},
		"chapter" : {
			"type": "string",
			"description": "Chapter",
			"default": "Chapter"			
		},
		"txt" : {
			"type": "string",
			"description": "Text",	
			"default": "Hello World"
		}
	},
	"methods": {
		"setColor": {
			"description": "Sets the background color.",
			"parameters": [
				{
					"name": "newColor",
					"type": "string",
					"description": "The new background color"
				}
			],
			"body": "this.color = newColor;"
		},
		"getColor": {
			"returnType": "string",
			"description": "Returns the background color.",
			"body": "return this.color;"
		},
		"setH1": {
			"description": "Sets the h1.",
			"parameters": [
				{
					"name": "newH1",
					"type": "string",
					"description": "The new title"
				}
			],
			"body": "this.title = newH1;"
		},
		"setH2": {
			"description": "Sets the h2.",
			"parameters": [
				{
					"name": "newH2",
					"type": "string",
					"description": "The new chapter"
				}
			],
			"body": "this.chapter = newH2;"
		},
		"setH3": {
			"description": "Sets the h3.",
			"parameters": [
				{
					"name": "newH3",
					"type": "string",
					"description": "The new text"
				}
			],
			"body": "this.txt = newH3;"
		}
	},
	"events": {
		"onClick": {
			"description": "Called when the user clicks the Box."
		}
	}
}